

<div class="container">

<div class="jumbotron">
  <div id="jumbowelcome" class="container">
    <h1 style="color: black;">Welcome <%= "#{current_user.first_name} #{current_user.last_name}" %></h1>

  </div>
  <br>
  <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a></p>

</div>
<div class="homepage-search">
<div class="form-section">
  <div class="row">
    <%= form_for :search, url: search_path, :method => 'get' do |f| %>
      <p>
        <div class="col-md-4"><%= f.text_field :location, placeholder: "Where to?", id: "whereto", class: "form-control"%></div>
        <div class="col-md-2"><%= f.text_field :bookingstartdate, id: 'bookingstartdate', placeholder: "Check-in Date", class: "form-control" %></div>
        <div class="col-md-2"><%= f.text_field :bookingenddate, id: 'bookingenddate', placeholder: "Check-out Date", class: "form-control" %></div>
        <div class="col-md-2"><%= f.select :no_guest, [ '1 Guest','2 Guest','3 Guest','4 Guest','5 Guest','6 Guest','6+ Guest' ], :prompt => 'Guest No.', class: "dropdown-menu" %></div>
        <div class="col-md-2"><%= submit_tag "Search", name: nil, class: "btn btn-default btn-primary" %></div>
      </p>
    <% end %>
  </div>
</div>
</div>

<div>
<%= form_tag search_index_path, method: :get do %>
    <div class="form-group">
      <%= text_field_tag :query, params[:query], class: 'form-control typeahead', autocomplete: "off" %>
      <%= submit_tag 'Search', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
